    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.0.0",
    "typeorm": "^0.3.9",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.4.7",
    "@types/express": "^4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
