    "@nestjs/terminus": "8.0.0",
    "@nestjs/typeorm": "^8.0.0",
    "@types/geojson": "^7946.0.10",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
