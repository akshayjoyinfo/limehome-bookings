    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "coveragePathIgnorePatterns": [
      "./dist/",
      "./src/global/pipes/",
      ".module.ts",
      "./src/utils/",
      "./src/main.ts",
      ".command.ts",
      "index.ts",
      ".entity.ts",
      ".model.ts",
      ".dto.ts",
      ".query.ts",
      ".filter.ts"
    ],
